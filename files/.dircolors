# 00=none 01=bold 04=underscore 05=blink 07=reverse 08=concealed
# 30=black 31=red 32=green 33=yellow 34=blue 35=magenta 36=cyan 37=white
# 40=black 41=red 42=green 43=yellow 44=blue 45=magenta 46=cyan 47=white

RESET                 0         # reset to "normal" color
DIR                      34     # directory
EXEC                     31     # executable
LINK                     35     # symbolic link
MULTIHARDLINK         01        # regular file with more than one link
FIFO                     33     # pipe
SOCK                     32     # socket

DOOR                  01;35     # door
BLK                      34;46  # block device driver
CHR                      34;43  # character device driver
ORPHAN                01;31;40  # symlink to nonexistent file
SETUID                   30;41  # file that is setuid (u+s)
SETGID                   30;46  # file that is setgid (g+s)
CAPABILITY            01;33;41  # file with capability
OTHER_WRITABLE           30;43  # dir that is other-writable (o+w)
STICKY_OTHER_WRITABLE    30;42  # dir that is sticky and other-writable (+t,o+w)
STICKY                   37;44  # dir that is sticky (+t) and not other-writable

